#include<bits/stdc++.h>
using namespace std;
int main(){
    int n, count_A=0, count_D=0;
    cin>>n;
    string s;
    cin>>s;
    for(int i=0;i<n;i++){
        if(s[i]=='A') count_A++;
        if(s[i]=='D') count_D++;
    }
    if(count_A>count_D) cout<<"Anton"<<endl;
    if(count_D>count_A) cout<<"Danik"<<endl;
    if(count_A==count_D) cout<<"Friendship"<<endl;
    return 0;
}
