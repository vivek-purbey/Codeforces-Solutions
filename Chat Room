#include<iostream>
using namespace std;
int main(){
    string s, str="hello";
    cin>>s;
    int j=0, count=0;
    for(int i=0;i<s.size();i++){
        if(s[i]==str[j]){
            j++;
            count++;
            if(count==5) break;
        }
    }
    if(count==5) cout<<"YES";
    else cout << "NO";
    return 0;
}
