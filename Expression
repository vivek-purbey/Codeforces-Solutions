#include<bits/stdc++.h>
using namespace std;
int main(){
    int a, b, c; cin>>a>>b>>c;
    int res, res1, res2, res3, res4, res5, res6, res7, res8;
    vector<int>v;
    res1=a+b+c;
    v.push_back(res1);
    res2=a*b*c;
    v.push_back(res2);
    res3=a+b*c;
    v.push_back(res3);
    res4=a*b+c;
    v.push_back(res4);
    res5=(a+b)*c;
    v.push_back(res5);
    res6=a+(b*c);
    v.push_back(res6);
    res7=(a*b)+c;
    v.push_back(res7);
    res8=a*(b+c);
    v.push_back(res8);
    sort(v.begin(),v.end());
    res=v.size();
    cout<<v[res-1];
    return 0;
}
