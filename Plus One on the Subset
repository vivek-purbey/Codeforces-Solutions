#include<bits/stdc++.h>
using namespace std;
int main() {
    int tc; cin>>tc; while(tc--) {
        int num; cin>>num;
        int a[num];
        int mn=INT_MAX;
        int mx=INT_MIN;
        for(int i=0;i<num;i++) {
            cin>>a[i];
            mn=min(mn,a[i]);
            mx=max(mx,a[i]);
        }
        cout<<abs(mx-mn)<<endl;
    }
    return 0;
}
